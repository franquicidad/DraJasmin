<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.apps.franco.drajasmin">

    <application
        android:allowBackup="true"
        android:icon="@drawable/logotrueskinappmodificado"
        android:label="@string/app_name"
        android:roundIcon="@drawable/logotrueskinappmodificado"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Info" />
        <activity android:name=".Productos" />
        <activity android:name=".Peeling"
            android:parentActivityName=".Productos" >
            <!-- Parent activity meta-data to support 4.0 and lower -->
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".Productos" />
        </activity>
        <activity android:name=".Plasma"
            android:parentActivityName=".Productos" >
            <!-- Parent activity meta-data to support 4.0 and lower -->
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".Productos" />
        </activity>
        <activity android:name=".Renovacion"
            android:parentActivityName=".Productos" >
            <!-- Parent activity meta-data to support 4.0 and lower -->
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".Productos" /></activity>
        <activity
            android:name=".Limpieza"
            android:parentActivityName=".Productos" >
        <!-- Parent activity meta-data to support 4.0 and lower -->
        <meta-data
            android:name="android.support.PARENT_ACTIVITY"
            android:value=".Productos" />
        </activity>
        <activity
            android:name=".Hibraderma"
            android:parentActivityName=".Productos" >
            <!-- Parent activity meta-data to support 4.0 and lower -->
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".Productos" />
        </activity>

    </application>

</manifest>